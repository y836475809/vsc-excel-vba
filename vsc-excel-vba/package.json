{
  "publisher": "y836475809",
  "name": "vsc-excel-vba",
  "displayName": "vsc-excel-vba",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/y836475809/vsc-excel-vba.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:vbaproject.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "vsc-excel-vba",
      "properties": {
        "vsc-excel-vba.loadDefinitionFiles": {
          "type": "boolean",
          "default": true,
          "description": "load d.vb files"
        },
        "vsc-excel-vba.enableLSP": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable VBA LSP"
        },
        "vsc-excel-vba.VBALanguageServerPort": {
          "type": "number",
          "default": 9088,
          "description": "server port"
        },
        "vsc-excel-vba.VBALanguageServerPath": {
          "type": ["string", "null"],
          "default": null,
          "description": "VBA language server executable file path"
        },
        "vsc-excel-vba.enableAutoLaunchVBALanguageServer": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable auto launch VBA language server"
        },
        "vsc-excel-vba.enableVBACompileAfterImport": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable run VBA compile after import"
        }
      }
    },
    "languages": [
      {
        "id": "vb",
        "extensions": [
          ".bas"
        ]
      },
      {
        "id": "vb",
        "extensions": [
          ".cls"
        ]
      }
    ],
    "commands": [
      {
        "command": "vsc-excel-vba.toggle",
        "title": "toggle"
      },
      {
        "command": "vsc-excel-vba.start",
        "title": "vsc-excel-vba start"
      },
      {
        "command": "vsc-excel-vba.stop",
        "title": "vsc-excel-vba stop"
      },
      {
        "command": "vsc-excel-vba.test.registerProvider",
        "title": "registerProvider command for test"
      },
      {
        "command": "vsc-excel-vba.createProject",
        "title": "Create vba project"
      },
      {
        "command": "vsc-excel-vba.import",
        "title": "import"
      },
      {
        "command": "vsc-excel-vba.export",
        "title": "export"
      },
      {
        "command": "vsc-excel-vba.compile",
        "title": "compile"
      },
      {
        "command": "vsc-excel-vba.gotoVBA",
        "title": "gotoVBA"
      },
      {
        "command": "vsc-excel-vba.gotoVSCode",
        "title": "gotoVSCode"
      },
      {
        "command": "vsc-excel-vba.runVBAProc",
        "title": "Run VBA procedure"
      },
      {
        "command": "vsc-excel-vba.openSheet",
        "title": "openSheet"
      },
      {
        "command": "vsc-excel-vba.setBreakpoints",
        "title": "Set breakpoints to VBA"
      },
      {
        "command": "vsc-excel-vba.newClassFile",
        "title": "New VBA class file"
      },
      {
        "command": "vsc-excel-vba.newModuleFile",
        "title": "New VBA module file"
      }
    ],
    "keybindings": [
      {
        "command": "vsc-excel-vba.import",
        "key": "ctrl+shift+i",
        "when": "resourceExtname == .bas || resourceExtname == .cls"
      },
      {
        "command": "vsc-excel-vba.runVBAProc",
        "key": "f5",
        "when": "editorTextFocus && resourceExtname == .bas"
      }
    ],
    "breakpoints": [
      {
        "language": "vb"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .xlsm",
          "command": "vsc-excel-vba.createProject",
          "group": "myGroup"
        },
        {
          "when": "resourceExtname == .xlsm",
          "command": "vsc-excel-vba.export",
          "group": "myGroup"
        },
        {
          "when": "resourceExtname == .csv",
          "command": "vsc-excel-vba.openSheet",
          "group": "myGroup"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "vsc-excel-vba.newClassFile",
          "group": "myGroup"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "vsc-excel-vba.newModuleFile",
          "group": "myGroup"
        }
      ],
      "editor/context": [
        {
          "command": "vsc-excel-vba.import",
          "when": "resourceExtname == .bas || resourceExtname == .cls",
          "group": "myGroup@1"
        },
        {
          "command": "vsc-excel-vba.gotoVBA",
          "when": "resourceExtname == .bas || resourceExtname == .cls",
          "group": "myGroup@2"
        },
        {
          "command": "vsc-excel-vba.gotoVSCode",
          "when": "resourceExtname == .bas || resourceExtname == .cls",
          "group": "myGroup@3"
        },
        {
          "command": "vsc-excel-vba.runVBAProc",
          "when": "resourceExtname == .bas",
          "group": "myGroup@4"
        },
        {
          "command": "vsc-excel-vba.setBreakpoints",
          "when": "resourceExtname == .bas || resourceExtname == .cls",
          "group": "myGroup@5"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "unittest": "mocha --ui tdd ./out/test/unittest/*.test.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "vscode-uri": "^3.0.7"
  }
}
