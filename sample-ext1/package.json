{
  "publisher": "y836475809",
  "name": "sample-ext1",
  "displayName": "sample-ext1",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sample-ext1.start",
    "onCommand:sample-ext1.startLanguageServer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vb",
        "extensions": [
          ".bas"
        ]
      },
      {
        "id": "vb",
        "extensions": [
          ".cls"
        ]
      }
    ],
    "commands": [
      {
        "command": "sample-ext1.start",
        "title": "Hello World"
      },
      {
        "command": "sample-ext1.startLanguageServer",
        "title": "startLanguageServer"
      },
      {
        "command": "sample-ext1.stopLanguageServer",
        "title": "stopLanguageServer"
      },
      {
        "command": "sample-ext1.renameFiles",
        "title": "renameFiles"
      },
      {
        "command": "testView.addEntry",
        "title": "Add"
      },
      {
        "command": "testView.myCommand",
        "title": "testView reg"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "testView",
          "name": "Test View"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "testView.addEntry",
          "when": "view == testView",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "!explorerResourceIsFolder",
          "command": "testView.myCommand",
          "group": "myGroup"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.8",
    "vscode-uri": "^3.0.7"
  }
}
