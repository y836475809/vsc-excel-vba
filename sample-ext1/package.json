{
  "publisher": "y836475809",
  "name": "sample-ext1",
  "displayName": "sample-ext1",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sample-ext1.start",
    "onCommand:sample-ext1.startLanguageServer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Sample-ext1",
      "properties": {
        "sample-ext1.serverPort": {
          "type": "number",
          "default": 9088,
          "description": "server port"
        },
        "sample-ext1.loadDefinitionFiles": {
          "type": "boolean",
          "default": true,
          "description": "load d.vb files"
        },
        "sample-ext1.serverExeFilePath": {
          "type": ["string", "null"],
          "default": null,
          "description": "LSP server file path"
        },
        "sample-ext1.autoLaunchServerApp": {
          "type": "boolean",
          "default": true,
          "description": "auto launch server app"
        }
      }
    },
    "languages": [
      {
        "id": "vb",
        "extensions": [
          ".bas"
        ]
      },
      {
        "id": "vb",
        "extensions": [
          ".cls"
        ]
      }
    ],
    "commands": [
      {
        "command": "sample-ext1.start",
        "title": "sample-ext1 start",
        "icon": "$(run)",
        "enablement": "sample-ext1.start.enable"
      },
      {
        "command": "sample-ext1.stop",
        "title": "sample-ext1 stop",
        "icon": "$(stop)"
      },
      {
        "command": "sample-ext1.startLanguageServer",
        "title": "startLanguageServer"
      },
      {
        "command": "sample-ext1.stopLanguageServer",
        "title": "stopLanguageServer"
      },
      {
        "command": "sample-ext1.renameFiles",
        "title": "renameFiles"
      },
      {
        "command": "sample-ext1.deleteFiles",
        "title": "deleteFiles"
      },
      {
        "command": "testView.myCommand",
        "title": "testView reg"
      },
      {
        "command": "sample-ext1.import",
        "title": "import"
      },
      {
        "command": "sample-ext1.export",
        "title": "export"
      },
      {
        "command": "sample-ext1.compile",
        "title": "compile"
      },
      {
        "command": "sample-ext1.gotoVBA",
        "title": "gotoVBA"
      },
      {
        "command": "sample-ext1.gotoVSCode",
        "title": "gotoVSCode"
      },
      {
        "command": "sample-ext1.runVBASubProc",
        "title": "Run Sub proc"
      },
      {
        "command": "sample-ext1.runViewItem",
        "title": "Run",
        "icon": "$(run)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "testView",
          "name": "Test View"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sample-ext1.start",
          "when": "view == testView",
          "group": "navigation"
        },
        {
          "command": "sample-ext1.stop",
          "when": "view == testView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "sample-ext1.runViewItem",
          "when": "view == testView",
          "group": "inline"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceExtname == .xlsm",
          "command": "testView.myCommand",
          "group": "myGroup"
        }
      ],
      "editor/context": [
        {
          "command": "sample-ext1.gotoVBA",
        },
        {
          "command": "sample-ext1.runVBASubProc",
          "when": "resourceExtname == .bas",
          "group": "myGroup"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.8",
    "vscode-uri": "^3.0.7"
  }
}
